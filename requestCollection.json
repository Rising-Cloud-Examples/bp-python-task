{
	"info": {
		"name": "bp-python-task",
		"description": "Test collection for the bp-python-task Rising Cloud Template",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Send Job",
			"request": {
				"method": "POST",
				"header": [
					{
						"warning": "This is a duplicate header and will be overridden by the X-RisingCloud-Auth header generated by Postman.",
						"key": "X-RisingCloud-Auth",
						"value": "{{auth}}",
						"type": "text"
					},
					{
						"key": "Content-Type",
						"value": "application/json",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"testAttribute\": \"testValue\"\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{host}}/risingcloud/jobs",
					"host": [
						"{{host}}"
					],
					"path": [
						"risingcloud",
						"jobs"
					]
				}
			},
			"response": []
		},
		{
			"name": "Get Job",
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "X-RisingCloud-Auth",
						"value": "{{auth}}",
						"type": "text"
					}
				],
				"url": {
					"raw": "{{host}}/risingcloud/jobs/:id",
					"host": [
						"{{host}}"
					],
					"path": [
						"risingcloud",
						"jobs",
						":id"
					]
				}
			},
			"response": []
		},
		{
			"name": "Kill Job",
			"request": {
				"method": "DELETE",
				"header": [
					{
						"key": "X-RisingCloud-Auth",
						"value": "{{auth}}",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"testAttribute\": \"testValue\"\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{host}}/risingcloud/jobs/:id",
					"host": [
						"{{host}}"
					],
					"path": [
						"risingcloud",
						"jobs",
						":id"
					]
				}
			},
			"response": []
		}
	],
	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					""
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					""
				]
			}
		}
	],
	"variable": [
		{
			"key": "host",
			"value": "https://bp-python-task.risingcloud.app",
			"type": "string"
		},
		{
			"key": "auth",
			"value": "",
			"type": "string"
		}
	]
}